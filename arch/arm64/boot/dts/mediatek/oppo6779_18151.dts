/*VENDOR_EDIT motor start*/
/*Fei.Mo@PSW.BSP.Sensor 2018-03-24, Add for motor direction switch*/
&pio {
    boost: boost {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO101__FUNC_GPIO101>;
            slew-rate = <1>;
            bias-pull-down; //pull down
            output-low; //digital output, no invert
            drive-strength = <3>;//0.15
        };
    };

    pwm_config: pwm_config {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO26__FUNC_PWM_0>;
            slew-rate = <1>;
            bias-disable;
            drive-strength = <0>;
        };
    };

    pwm_config_as_gpio: pwm_config_as_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO26__FUNC_GPIO26>;
            slew-rate = <1>;
            bias-disable;
			output-low;
            drive-strength = <0>; //LOW strength
        };
    };

    enable_gpio: enable_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO69__FUNC_GPIO69>;
            slew-rate = <1>;
            bias-disable; //No Pull
			output-low;
            drive-strength = <0>; //LOW strength
        };
    };

    dir_gpio: dir_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO102__FUNC_GPIO102>;
			slew-rate = <1>;
            bias-disable;
			output-low;
            drive-strength = <0>; /* LOW strength */
        };
    };

    standby_gpio: standby_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO88__FUNC_GPIO88>;
            slew-rate = <1>;
            bias-disable;
            output-low; /* digital output, no invert */
            drive-strength = <0>; /* LOW strength */
        };
    };

    mode1_gpio: mode1_gpio {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO16__FUNC_GPIO16>;
			slew-rate = <1>;
            bias-disable;
			output-low;
            drive-strength = <0>; /* LOW strength */
        };
    };
};

&motor_pl {
	status = "okay";
	compatible = "oppo-motor";
	hall-irq-pos = <170>;
	motor-retard-pos = <430>;
	detect-delay = <10>;//10ms
	distance = <83>;//8.3mm
	speed-up-distance = <15>;//0.5mm
	speed-down-distance = <2>;//0.2mm
	brake-speed = <156250>;
	pulse-speed = <12000>;
	md-speed = <12000>;
	stop-neg-point = <3>;
	stop-pos-point = <3>;
	hall-data-sign = <0>;//1 represent positive ,0 represent negative
};

&motor_drv {
	status = "okay";
	compatible = "motor_drv-220";
	pinctrl-names = "boost","pwm_config","pwm_config_as_gpio","enable_gpio","dir_gpio","standby_gpio","mode1_gpio";
	pinctrl-0 = <&boost>;
	pinctrl-1 = <&pwm_config>;
	pinctrl-2 = <&pwm_config_as_gpio>;
	pinctrl-3 = <&enable_gpio>;
	pinctrl-4 = <&dir_gpio>;
	pinctrl-5 = <&standby_gpio>;
	pinctrl-6 = <&mode1_gpio>;
	boost-gpio = <&pio 101 0x00>;
	standby-gpio = <&pio 88 0>;
	dir-gpio = <&pio 102 0>;
	enable-gpio =  <&pio 69 0>;
	step-gpio = <&pio 26 0>;
	mode1-gpio = <&pio 15 0>;
	ratio-a = <24>;
	ratio-b = <1840>;//18.40
	ratio-c = <20>;
	ratio-d = <32>;
};

&i2c3 {
    dhall0-m1120@0f {
        status = "okay";
        compatible = "oppo,dhall-m1120";
        reg = <0x0f>;
        pinctrl-names = "hall_power_down","hall_power_up","hall0_interrupt_input";
        pinctrl-0 = <&hall_power_down>;
        pinctrl-1 = <&hall_power_up>;
        pinctrl-2 = <&hall0_interrupt_input>;
        interrupt-parent = <&pio>;
        interrupts = <13 IRQ_TYPE_LEVEL_LOW 13 0>;
        dhall,irq-gpio = <&pio 13 0x2008>;
        dhall,id = <0>;
        hall-power-gpio = <&pio 155 0>;
	};

    dhall1-m1120@0c {
        status = "okay";
        compatible = "oppo,dhall-m1120";
        reg = <0x0c>;
        pinctrl-names = "hall_power_down","hall_power_up";
        pinctrl-0 = <&hall_power_down>;
        pinctrl-1 = <&hall_power_up>;
        dhall,id = <1>;
        hall-power-gpio = <&pio 155 0>;
	};
};

&pio {
    hall_power_up: hall_power_up {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO155__FUNC_GPIO155>;
            slew-rate = <1>;
            drive-strength = <2>;
            output-high;
            bias-pull-down;
		};
	};
    hall_power_down: hall_power_down {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO155__FUNC_GPIO155>;
            slew-rate = <1>;
            drive-strength = <2>;
            output-low;
            bias-pull-down;
        };
    };
    hall0_interrupt_input: hall0_interrupt_input {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO13__FUNC_GPIO13>;
            slew-rate = <0>;
            drive-strength = <2>;
            bias-disable; //No Pull
        };
    };
};
/*motor end*/

